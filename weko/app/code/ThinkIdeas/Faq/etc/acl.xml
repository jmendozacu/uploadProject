<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="ThinkIdeas_Faq::faq" title="FAQ" sortOrder="30">
                    <resource id="ThinkIdeas_Faq::category" title="Manage Category" sortOrder="1">
                    </resource>
                    <resource id="ThinkIdeas_Faq::faq_faq" title="Manage FAQ" sortOrder="2">
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="ThinkIdeas_Faq::config_faq" title="FAQ" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
